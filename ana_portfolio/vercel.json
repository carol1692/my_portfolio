{
  "version": 2,
  "builds": [
    {
      "src": "my_portfolio/ana_portfolio/ana_portfolio/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "my_portfolio/ana_portfolio/ana_portfolio/wsgi.py"
    }
  ]
}
